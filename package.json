{
  "name": "ag-colbuilder",
  "version": "0.1.0",
  "description": "A fluent, strongly typed column definition builder for AG Grid.",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "lint": "bun x biome check",
    "format": "bun x biome format --write",
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "prepare": "bun run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ethanhann/ag-colbuilder.git"
  },
  "bugs": {
    "url": "https://github.com/ethanhann/ag-colbuilder/issues"
  },
  "homepage": "https://github.com/ethanhann/ag-colbuilder#readme",
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@types/bun": "latest",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "4.0.14",
    "expect-type": "^1.2.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "peerDependencies": {
    "ag-grid-community": ">=34.0.0"
  }
}
